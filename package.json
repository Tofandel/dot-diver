{
  "name": "@clickbar/dot-diver",
  "version": "1.0.1",
  "description": "Types and utilities to access object properties by dot notation.",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "build": "vite build",
    "test": "vitest run && vitest typecheck --run",
    "lint": "eslint ./ --ext=js,ts",
    "prettier": "prettier . --cache",
    "fix": "pnpm lint --fix && pnpm format",
    "format": "pnpm prettier --write"
  },
  "keywords": [
    "ts",
    "types",
    "typescript",
    "util",
    "utility",
    "utilities",
    "dot",
    "path",
    "dot-path"
  ],
  "author": "clickbar.",
  "repository": "https://github.com/clickbar/dot-diver.git",
  "bugs": {
    "url": "https://github.com/clickbar/dot-diver/issues"
  },
  "license": "MIT",
  "maintainers": [
    "Daniel Thoma <dt@clickbar.dev> (https://github.com/djfhe)",
    "Tobias Kr√∂ll <tk@clickbar.dev> (https://github.com/saibotk)"
  ],
  "devDependencies": {
    "@clickbar/eslint-config-typescript": "^8.0.2",
    "eslint": "^8.47.0",
    "prettier": "^3.0.2",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vite-plugin-dts": "^3.5.2",
    "vitest": "^0.34.2"
  }
}
