{
  "name": "@clickbar/dot-diver",
  "version": "1.0.0",
  "description": "Types and utilities to access object properties by dot notation.",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "build": "vite build",
    "test": "vitest run && vitest typecheck --run",
    "lint": "eslint ./ --ext=js,ts",
    "fix": "eslint ./ --ext=js,ts --fix"
  },
  "keywords": [
    "ts",
    "types",
    "typescript",
    "util",
    "utility",
    "utilities",
    "dot",
    "path",
    "dot-path"
  ],
  "author": "clickbar.",
  "license": "MIT",
  "maintainers": [
    "Daniel Thoma <dt@clickbar.dev> (https://github.com/djfhe)",
    "Tobias Kr√∂ll <tk@clickbar.dev> (https://github.com/saibotk)"
  ],
  "devDependencies": {
    "@clickbar/eslint-config-typescript": "^4.3.0",
    "eslint": "^8.36.0",
    "prettier": "^2.8.6",
    "typescript": "^5.0.2",
    "vite": "^4.2.1",
    "vite-plugin-dts": "^2.1.0",
    "vitest": "^0.29.7"
  }
}